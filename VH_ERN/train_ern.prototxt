name: "ERN for VH-data"

###############################################
## DATA Layers
###############################################

## Training phase

layer{
  name: "data_rgb"
  type: "Data"
  top: "data_rgb"
  include {
    phase: TRAIN
  }
  transform_param {
    mean_value: 73.83527
    mean_value: 74.98086
    mean_value: 108.47272
  }
  data_param {
    source: "/media/lh/D/VH_Data/train/VH_train_pre/rgb_DB"
    batch_size: 2
    backend: LMDB
  }
}

layer{
  name: "data_ndsm"
  type: "Data"
  top: "data_ndsm"
  include {
    phase: TRAIN
  }
  transform_param {
    mean_value: 128.77069
  }
  data_param {
    source: "/media/lh/D/VH_Data/train/VH_train_pre/ndsm_DB"
    batch_size: 2
    backend: LMDB
  }
}

layer {
  name: "label"
  type: "Data"
  top: "label"
  include {
    phase: TRAIN
