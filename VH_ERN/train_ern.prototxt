  data_param {
    source: "/media/lh/D/VH_Data/train/VH_train_pre/edge_png_DB"
    batch_size: 2
    backend: LMDB
  }
}


## Validation phase

layer {
  name: "data_rgb"
  type: "Data"
  top: "data_rgb"
  include {
    phase: TEST
  }
  transform_param {
    mean_value: 73.83527
    mean_value: 74.98086
    mean_value: 108.47272
  }
  data_param {
    source: "/media/lh/D/VH_Data/test/rgb_DB"
    batch_size: 2
    backend: LMDB
  }
}

layer {
  name: "data_ndsm"
  type: "Data"
  top: "data_ndsm"
